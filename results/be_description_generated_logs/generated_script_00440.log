  using lattice units in orthogonal box = (0 0 -0.84515425) to (16.903085 16.903085 0.84515425)
  create_atoms CPU = 0.000 seconds

# Set mass and dipole moments
mass 1 1.0
set type 1 dipole/random 0.75 12345
