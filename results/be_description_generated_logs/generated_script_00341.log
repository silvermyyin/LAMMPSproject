mass            1 1.0
mass            2 1.0

# Define interactions
pair_style      lj/cut/coul/long 12.5
pair_coeff      1 1 1.0 1.0
pair_coeff      2 2 1.0 1.0
pair_coeff      1 2 1.0 1.0

